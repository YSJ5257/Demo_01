<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>LoginCheck</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
 <filter>
    <display-name>AllFlter</display-name>
    <filter-name>AllFlter</filter-name>
    <filter-class>com.ysj.filter.AllFlter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AllFlter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>LoginFilter</display-name>
    <filter-name>LoginFilter</filter-name>
    <filter-class>com.ysj.filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/other.jsp</url-pattern>
    <url-pattern>/one.jsp</url-pattern>
    <url-pattern>/two.jsp</url-pattern>
  </filter-mapping>
   <error-page>
   <error-code>500</error-code>
  <location>/error.jsp</location>
   </error-page>
</web-app>